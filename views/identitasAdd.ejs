<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/bootstrap/js/bootstrap.min.js" />
    <link rel="stylesheet" href="/css/home.css" />
  </head>
  <body>
    <%- include('navbar'); %>

    <div class="main-content" id="main-content">
      <div class="container">
        <h1>Add Identitas Sekolah</h1>

        <form id="create-identitas-form">
          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Nama Sekolah</label>
            <input
              type="text"
              class="form-control"
              id="strNamaSekolah"
              placeholder="masukan nama sekolah"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Nomor Induk Sekolah</label>
            <input
              type="text"
              class="form-control"
              id="strNIS"
              placeholder="masukan nomor induk sekolah"
            />
          </div>
          
  
          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlTextarea1">Alamat</label>
            <textarea
              class="form-control"
              id="strAlamat"
              placeholder="Masukkan alamat"
              rows="4" 
            ></textarea>
          </div>
          
          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Kode POS</label>
            <input
              type="text"
              class="form-control"
              id="strKodePos"
              placeholder="masukan kode pos"
            />
          </div>
          
          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Nomor Telepon</label>
            <input
              type="text"
              class="form-control"
              id="strTelpon"
              placeholder="masukan nomor telpon"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Kelurahan</label>
            <input
              type="text"
              class="form-control"
              id="strKelurahan"
              placeholder="masukan kelurahan"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Kecamatan</label>
            <input
              type="text"
              class="form-control"
              id="strKecamatan"
              placeholder="masukan kecamatan"
            />
          </div>
          
          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Kabupaten</label>
            <input
              type="text"
              class="form-control"
              id="strKabupaten"
              placeholder="masukan kabupaten"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Provinsi</label>
            <input
              type="text"
              class="form-control"
              id="strProvinsi"
              placeholder="masukan provinsi"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px; margin-top: 30px">
            <label for="exampleFormControlInput1">Website</label>
            <input
              type="text"
              class="form-control"
              id="strWebsite"
              placeholder="masukan website"
            />
          </div>

          <div class="form-group" style="margin-bottom: 15px">
            <div class="form-group">
              <label for="email">Email:</label>
              <input
                type="email"
                class="form-control"
                id="strEmail"
                name="email"
                required
              />
            </div>
          </div>




       
          <button type="submit" class="btn btn-primary float-right">
            Tambah Identitas Sekolah
          </button>
        </form>
      </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.3/dist/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

    <!-- script controll library tinymce -->
    <script src="/js/index.js"></script>

  </body>
</html>
